#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    int a0; 
    int a1; 
    int a2; 
    scanf("%d %d %d", &a0, &a1, &a2);
    int b0; 
    int b1; 
    int b2; 
    scanf("%d %d %d", &b0, &b1, &b2);
    // Write Your Code Here
    int a,b,c,ar[2];
    ar[0]=0,ar[1]=0;
    if(a0>b0)
        ar[0]=ar[0]+1;
    if(a1>b1)
        ar[0]=ar[0]+1;
    if(a2>b2)
        ar[0]=ar[0]+1;
    if(a0<b0)
       ar[1]=ar[1]+1;
    if(a1<b1)
        ar[1]=ar[1]+1;
    if(a2<b2)
        ar[1]=ar[1]+1;
    if(a0==b0||a1==b1||a2==b2)
    {    ar[0]=ar[0]+0;
         ar[1]=ar[1]+0;
    }
    
            printf("%d %d",ar[0],ar[1]);
  
    return 0;
}
